<p id="notice"><%= notice %></p>

<h1>Company Profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Company name</th>
      <th>Logo</th>
      <th>Description</th>
      <th>Street</th>
      <th>Suburb</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Company culture</th>
      <th>Company type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_profiles.each do |company_profile| %>
      <tr>
        <td><%= company_profile.first_name %></td>
        <td><%= company_profile.last_name %></td>
        <td><%= company_profile.phone %></td>
        <td><%= company_profile.company_name %></td>
        <td><%= company_profile.logo %></td>
        <td><%= company_profile.description %></td>
        <td><%= company_profile.street %></td>
        <td><%= company_profile.suburb %></td>
        <td><%= company_profile.postcode %></td>
        <td><%= company_profile.country %></td>
        <td><%= company_profile.latitude %></td>
        <td><%= company_profile.longitude %></td>
        <td><%= company_profile.company_culture %></td>
        <td><%= company_profile.company_type %></td>
        <td><%= company_profile.user %></td>
        <td><%= link_to 'Show', company_profile %></td>
        <td><%= link_to 'Edit', edit_company_profile_path(company_profile) %></td>
        <td><%= link_to 'Destroy', company_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Profile', new_company_profile_path %>
